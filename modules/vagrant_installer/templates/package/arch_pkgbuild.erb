# This is an example PKGBUILD file. Use this as a start to creating your own,
# and remove these comments. For more information, see 'man PKGBUILD'.
# NOTE: Please fill out the license field for your package! If it is unknown,
# then please put 'unknown'.

# Maintainer: HashiCorp <biz@hashicorp.com>
pkgname=<%= @pkgname %>
pkgver="<%= @pkgver %>"
pkgrel=1
epoch=
pkgdesc=""
arch=('<%= @hardwaremodel %>')
url=""
license=('MIT')
groups=()
depends=()
makedepends=()
checkdepends=()
optdepends=()
provides=()
conflicts=()
replaces=()
backup=()
options=(!strip)
install=
changelog=
source=('<%= @source_package %>')
noextract=()
md5sums=(`md5sum <%= @source_package %> | awk '{ print $1 }'`)

build() {
  echo build
}

check() {
  echo check
}

package() {
  mv <%= @staging_dir %>/* $pkgdir
}
